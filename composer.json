{
    "name": "oktopuce/site-generator",
    "type": "typo3-cms-extension",
    "description": "Site generator / tree model duplicator used to generate mini-website or duplicate tree model",
    "homepage": "https://www.oktopuce.fr",
    "license": ["GPL-2.0-or-later"],
	"version": "3.2.2",
	"keywords": [
        "TYPO3 CMS",
        "extension",
        "wizard",
        "backend"
    ],
    "authors": [
      {
        "name": "Florian Rival",
		"email": "florian.typo3@oktopuce.fr",
        "homepage": "https://www.oktopuce.fr"
      }
    ],
	"support": {
		"issues": "https://github.com/Oktopuce/site_generator/issues",
		"source": "https://github.com/Oktopuce/site_generator"
	},
	"repositories": [
		{
		  "type": "composer",
		  "url": "https://composer.typo3.org/"
		}
	],
	"require": {
        "typo3/cms-core": "^12 || ^13"
    },
    "autoload": {
        "psr-4": {
            "Oktopuce\\SiteGenerator\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "site_generator"
        }
    }
}
